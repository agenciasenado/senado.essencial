    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "superstatic": "^4.0.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "yaml-loader": "^0.4.0"
