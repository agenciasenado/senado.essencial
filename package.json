{
  "name": "senado.essencial",
  "version": "1.13.1",
  "repository": "http://git.io/senado.essencial",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "superstatic build -p 8080 --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress && grunt",
    "prebuild": "rimraf build",
    "deploy": "npm run build && gh-pages -d build"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "expose-loader": "^0.7.5",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "grunt": "^1.0.4",
    "grunt-banner": "^0.6.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-less": "^2.0.0",
    "grunt-contrib-pug": "^2.0.0",
    "grunt-he": "^0.1.0",
    "grunt-uncss": "^0.8.6",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "load-grunt-config": "^1.0.2",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "superstatic": "^6.0.4",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.4.1",
    "senado.css": "git+https://github.com/fabianonunes/senado.css.git#2.11.0"
  }
